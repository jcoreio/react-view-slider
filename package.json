    "gh-pages": "^2.0.1",
    "global-jsdom": "^9.0.1",
    "jsdom": "^22.1.0",
    "mocha": "^10.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-loader": "^3.1.3",
    "rimraf": "^2.6.0",
    "sinon": "^17.0.1",
    "typescript": "^5.2.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
